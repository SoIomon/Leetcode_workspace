    "class Solution:\n",
    "    def convertToTitle(self, n: int) -> str:\n",
    "        res = \"\"\n",
    "        while n != 0:\n",
    "            n -= 1\n",
    "            res += chr((n) % 26 + 65)\n",
    "            n //= 26\n",
    "        return res[::-1]"
   ]
